 	__gpio_as_input(GPIO_CHARG_STAT_N);
-	__gpio_as_input(GPIO_USB_DETE);
-
-	__gpio_as_output(GPIO_DISP_OFF_N);
 
-	__gpio_as_output(GPIO_LED_EN);
 }
 
